<h1 class="page-header"><%= source_application %></h1>

<h2><%= message_type %></h2>

  <table class="table table-hover parameters">
    <thead>
      <tr>
        <th>Name</th>
        <th>Value</th>
      </tr>
    </thead>

    <tbody>
      <% @parameters.each do |parameter| %>
        <tr>
          <td>
            <%= parameter.Name %>
          </td>
          <td>
            <%= parameter.Value %>
          </td>
        <% end %>
    </tbody>

  </table>

