<% breadcrumb :message %>

  <table class="table table-hover parameters">
    <thead>
      <tr>
        <th>Name</th>
        <th>Value</th>
      </tr>
    </thead>

    <tbody>
      <% @parameters.each do |parameter| %>
        <tr>
          <td>
            <%= parameter.Name %>
          </td>
          <td>
            <%= parameter.Value %>
          </td>
        <% end %>
    </tbody>

  </table>

  <%= link_to "Back", :back, :class => "btn btn-default" %>
